{% set theme_config = attribute(config.themes, config.system.pages.theme) %}
<!DOCTYPE html>
<html lang="{{ grav.language.getActive ?: theme_config.default_lang }}">
<head>
{% block head %}
    <meta charset="utf-8" />
    <title>{% if header.title %}{{ header.title|e('html') }} | {% endif %}{{ site.title|e('html') }}</title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% include 'partials/metadata.html.twig' %}

     <link rel="shortcut icon" type="image/x-icon" href="{{ url('theme://images/favicon.ico') }}">
    <link rel="apple-touch-icon" href="{{ url('theme://images/favicon.ico') }}"/>
    <link rel="canonical" href="{{ page.url(
    true, true) }}" />

    {% block stylesheets %}
        {% do assets.addCss('https://fonts.googleapis.com/icon?family=Material+Icons') %}
        {% do assets.addCss('https://fonts.googleapis.com/css?family=Lato:100,300,400,600,700,900') %}
        {% do assets.addCss('theme://css/css/normalize.css') %}
        {% do assets.addCss('theme://css/css/materialize.css') %}
        {% do assets.addCss('theme://css/css/main.css') %}
    {% endblock %}
    {{ assets.css() }}


{% endblock head %}
</head>
   <div id="top-nav">
      <img class="logo" src="{{ url('theme://images/logo.png') }}">
      <input id="burger" type="checkbox" />
      <label for="burger">
      <span class="bar"></span>
      <span class="bar"></span>
      <span class="bar"></span>
      </label>
      <nav>
      {# {% include 'partials/navigation.html.twig' %} #}
        <ul>
          <li>
            <a href="../home">Home</a>
          </li>
          <li>
            <a href="../about">About Next Gen</a>
            <ul class="dropdown">
                <li><a href="../about/programs">Programs</a></li>
                <li><a href="../about/team">Team</a></li>
            </ul>
          </li>
          <li>
            <a href="../community">Community</a>
            <ul class="dropdown">
                <li><a href="/community/job-board">Job Board</a></li>
                <li><a href="/community/job-description">Job Description</a></li>
            </ul>
          </li>
          <li>
            <a href="../initiatives">Initiatives</a>
            <ul class="dropdown">
                <li><a href="/initiatives/calendar">Laptops</a></li>
                <li><a href="/initiatives/events">Monitors</a></li>
            </ul>
          </li>
          <li>
            <a href="../get-involved">Get Involved</a>
            <ul class="dropdown">
                <li><a href="/get-involved/become-sponsor-1">Become a Sponsor</a></li>
                <li><a href="/get-involved/donate">Donate</a></li>
                <li><a href="/get-involved/volunteer">Volunteer</a></li>
            </ul>
          </li>
        </ul>
      </nav>
    </div>
    <main>
  {% block content %}
  {% endblock %}
    </main>
    <img class="responsive-tri" src="{{ url('theme://images/triangles.png') }}">
  <footer class="page-footer"></footer>




 {% block javascripts %}
        {% do assets.addJs('https://code.jquery.com/jquery-3.2.1.min.js', 100) %}
        {% do assets.addJs('theme://js/materialize.js', 99) %}
        {% do assets.addJs('https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js', 98) %}
        {% do assets.addJs('theme://js/scripts.js', 97) %}
        {% do assets.addJs('https://maps.googleapis.com/maps/api/js?key=YOURAPIKEY&callback=initMap', 89) %}

        {% do assets.addJs('https://use.fontawesome.com/30b4afc2a9.js', 90) %}
 
    {% endblock %}
    {{ assets.js() }}

  </body>
</html>
